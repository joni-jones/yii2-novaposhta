{
    "name": "webkadabra/yii2-novaposhta",
    "description": "Yii2 extension to work with Nova Poshta web api",
    "keywords": ["yii2","extension","api","novaposhta"],
    "type": "yii2-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Ievgen Sentiabov",
            "email": "ievgen.sentiabov@gmail.com"
        }
    ],
    "version": "0.0.6",
    "minimum-stability": "stable",
    "require": {
        "yiisoft/yii2": "2.*",
        "guzzlehttp/guzzle": "^6.0 || ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4"
    },
    "autoload": {
        "psr-4": {
            "jones\\novaposhta\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "jones\\novaposhta\\tests\\": "tests"
        }
    }
}
